{
    "name": "full_stack_authentication_system",
    "version": "1.0.0",
    "description": "Authentication System",
    "main": "src/server.ts",
    "scripts": {
        "build": "tsc",
        "dev": "dotenvx run -f .env.development -- nodemon",
        "start": "dotenvx run -f .env.production -- node dist/server.js",
        "start:env:encrypt": "dotenvx encrypt -f .env.production",
        "db:generate": "drizzle-kit generate --config=src/drizzle/drizzle.config.ts",
        "db:migrate": "drizzle-kit migrate --config=src/drizzle/drizzle.config.ts",
        "db:studio": "drizzle-kit studio --config=src/drizzle/drizzle.config.ts",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "format:check": "prettier . --check",
        "format:fix": "prettier . --write",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "Vraj Parikh",
    "license": "ISC",
    "type": "module",
    "dependencies": {
        "@dotenvx/dotenvx": "^1.38.4",
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "drizzle-orm": "^0.40.0",
        "express": "^4.21.2",
        "helmet": "^8.0.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "ms": "^2.1.3",
        "pg": "^8.13.3",
        "source-map-support": "^0.5.21",
        "winston": "^3.17.0",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.7.1",
        "@commitlint/config-conventional": "^19.7.1",
        "@eslint/js": "^9.21.0",
        "@tsconfig/node22": "^22.0.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/morgan": "^1.9.9",
        "@types/ms": "^2.1.0",
        "@types/pg": "^8.11.11",
        "@types/source-map-support": "^0.5.10",
        "drizzle-kit": "^0.30.5",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.2",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.3",
        "nodemon": "^3.1.9",
        "prettier": "3.5.3",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.0"
    }
}
